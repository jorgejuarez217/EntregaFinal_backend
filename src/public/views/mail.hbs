   <h2>Orden: {{order.orderNumber}} Estado: {{order.status}} a las {{order.timestamp}}</h2>
    <h3>e-mail:{{order.username}} Dirección:{{order.address}} </h3>
 <table class="table table-bordered border-dark" style="margin:3rem 0;">
  
        <thead class="thead-dark" style="background-color:aliceblue ;">
            
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Descripción</th>
                <th scope="col">Precio</th>
                <th scope="col">Cantidad</th>
            </tr>
        </thead>
        <tbody>
            {{#each order.productos}}

                <tr scope="row">
                    <td>
                            {{title}} 
                    </td>
                    <td>
                            {{description}}
                    </td>
                    <td>
                            {{price}}
                    </td>
                    <td>
                            {{cantidad}}   
                    </td>
                    
                </tr>  
            
            {{/each}}
        </tbody>
    </table>